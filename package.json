{
  "name": "ngx-order-pipe",
  "version": "3.0.0",
  "description": "Angular order pipe, order collection by a field",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test --watch false",
    "test:w": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "docs": "ng build --configuration production --output-path docs --base-href ngx-order-pipe && cp -R ./docs/browser/* ./docs/ && rm -rf ./docs/browser && cp ./docs/index.html ./docs/404.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VadimDez/ngx-order-pipe.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "order",
    "pipe",
    "sort",
    "filter"
  ],
  "author": "Vadym Yatsyuk <vadym.yatsyuk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VadimDez/ngx-order-pipe/issues"
  },
  "homepage": "https://github.com/VadimDez/ngx-order-pipe#readme",
  "private": false,
  "peerDependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular/animations": "^17.3.0",
    "@angular/cli": "^17.3.7",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/language-service": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript": "~5.4.2",
    "zone.js": "~0.14.3"
  }
}
