{
  "name": "ngx-order-pipe",
  "version": "2.2.0",
  "description": "Angular order pipe, order collection by a field",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test --watch false",
    "test:w": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p package.json",
    "docs": "ng build --prod --output-path docs --base-href ngx-order-pipe && cp ./docs/index.html ./docs/404.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VadimDez/ngx-order-pipe.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "order",
    "pipe",
    "sort",
    "filter"
  ],
  "author": "Vadym Yatsyuk <vadym.yatsyuk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VadimDez/ngx-order-pipe/issues"
  },
  "homepage": "https://github.com/VadimDez/ngx-order-pipe#readme",
  "private": false,
  "peerDependencies": {
    "@angular/core": ">=5.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/animations": "~10.1.3",
    "@angular/cli": "~10.1.3",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~10.1.3",
    "@angular/language-service": "~10.1.3",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.0.3",
    "protractor": "~7.0.0",
    "rxjs": "~6.6.0",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "zone.js": "~0.10.2"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts"
    }
  }
}
